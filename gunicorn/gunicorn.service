[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target
[Service]
User=ubuntu
Group=www-data
WorkingDirectory=/home/ubuntu/project-pipeline-aws
ExecStart=/home/ubuntu/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/ubuntu/project-pipeline-aws/aws_cicd/aws_cicd.sock aws_cicd.wsgi:application

       
[Install]
WantedBy=multi-user.target